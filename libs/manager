const Employee = require("./employee")

class Manager extends Employee {
    constructor(name,id,email,officenumber){
        super(name,id,email)
        this.officenumber = officenumber;
    }

    ManOfficeNumber (){
        return this.officenumber;
    }

    managerRole (){
        return "Manager"
    };
}
 module.exports = Manager;